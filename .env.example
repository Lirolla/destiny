# ============================================
# DESTINY HACKING — Environment Configuration
# ============================================

# Database (MySQL)
DATABASE_URL=mysql://user:password@host:3306/destiny_hacking

# Authentication
JWT_SECRET=your-jwt-secret-minimum-32-characters

# AI Coach — LLM Provider
# Options: "manus" (default), "openai", "anthropic"
LLM_PROVIDER=manus
LLM_MODEL=

# Manus Forge (default, only works in Manus environment)
BUILT_IN_FORGE_API_URL=https://forge.manus.im
BUILT_IN_FORGE_API_KEY=

# OpenAI (if LLM_PROVIDER=openai)
OPENAI_API_KEY=sk-...

# Anthropic (if LLM_PROVIDER=anthropic)
ANTHROPIC_API_KEY=sk-ant-...

# Text-to-Speech — Audiobook Generation
ELEVENLABS_API_KEY=

# App Configuration
NODE_ENV=production
VITE_APP_ID=destiny-hacking
OWNER_OPEN_ID=

# OAuth (Manus-specific)
OAUTH_SERVER_URL=
